class Parent {
    void display() {
        System.out.println("Parent");
    }
}

class Child extends Parent {
    void show() {
        System.out.println("Child");
    }
}

public class Main {
    public static void main(String[] args) {
        Parent obj1 = new Parent();
        obj1.display();
        
        Child obj2 = new Child();
        obj2.show();
    }
}
